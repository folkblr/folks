<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.creation.android.folkapp2019">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.SEND_SMS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/folk_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.creation.android.folkapp2019.AwatingApproval"></activity>
        <activity android:name="com.creation.android.folkapp2019.RequestAccomodationActivity" />
        <activity android:name="com.creation.android.folkapp2019.ConfirmOccupancy" />
        <activity android:name="com.creation.android.folkapp2019.Splash_screen" />
        <activity android:name="com.creation.android.folkapp2019.CalendarActivity" />
        <activity android:name="com.creation.android.folkapp2019.OTP_folk_member" />
        <activity android:name="com.creation.android.folkapp2019.OTP" />
        <activity android:name="com.creation.android.folkapp2019.Guest_signup" />
        <activity android:name="com.creation.android.folkapp2019.Folk_signup" />
        <activity android:name="com.creation.android.folkapp2019.LoginActivity" />
        <activity
            android:name="com.creation.android.folkapp2019.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>